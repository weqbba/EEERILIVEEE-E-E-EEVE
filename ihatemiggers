    ColorSequenceKeypoint.new(0, purpleColor),
    ColorSequenceKeypoint.new(0.5, Color3.fromRGB(100, 100, 100)),
    ColorSequenceKeypoint.new(1, purpleColor)
}

local Rq = Instance.new("UIGradient", Text2)
Rq.Color = H
Rq.Enabled = true
Rq.Rotation = 85
Rq.Offset = Vector2.new(-1, 0)
Rq.Name = "Test3"

